@model IEnumerable<CSE_Hankers.Models.UserFollowing>

@{
    ViewData["Title"] = "FollowedUsers";
}

<h1>FollowedUsers</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                <a asp-action="Edit" asp-route-id="@item.userFollowingId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.userFollowingId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.userFollowingId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
